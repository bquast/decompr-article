	Journal = {Journal of International Economics},
	Keywords = {Production sharing; GVCs; Trade; Vertical specialization; Domestic content; Value added; Input--output tables},
	Number = {2},
	Pages = {224-236},
	Title = {{Accounting for intermediates: Production sharing and trade in value added}},
